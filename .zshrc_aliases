# Docker compose commands aliases
alias dc="docker compose"
alias dcu="docker compose up"
alias dcd="docker compose down"

# Goggle cloud commands aliases
alias gcal="gcloud auth login"
alias gccl="gcloud config list"
alias gcpl="gcloud projects list"
alias gcsl="gcloud services list"

alias gcccl="gcloud config configurations list"

# Terraform commands aliases
alias tfi="terraform init"
alias tfa="terraform apply"
alias tfd="terraform destroy"
